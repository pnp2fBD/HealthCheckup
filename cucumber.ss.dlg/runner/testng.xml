<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="DLG - UXP - SS Test Suite" allow-return-values="true">
	<listeners>
		<listener class-name="listeners.CustomListeners"></listener>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>
	<test name="Self Service Test Pack" allow-return-values="true"
		preserve-order="false">
		<packages>
			<package name="cucumber.ss.dlg" />
		</packages>
		<classes>
			<class name="cucumber.ss.dlg.SSrunnerTest" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->